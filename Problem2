#include <iostream>
#include <conio.h>
#include <string>
#include <iomanip>
using namespace std;

int main()
{
	double Mass;
	double Density;

    cout << "Enter Your Mass in Grams:" << endl;
	cin >> Mass;
	cout << "Enter Your Density in Grams Per Cubic Centimeter:" << endl;
	cin >> Density;
	cout << fixed;
	cout << setprecision(2);
	cout << "Volume of the Object:	" << Mass / Density << endl;

	

	_getch();
	return 0;

}
